<!--index.wxml-->

<view class="catlist">
  <block wx:for="{{catlist}}">
  <navigator url="/pages/cats/{{item.name}}/{{item.name}}" >
    <view class="catitem">
        <image  class = 'avator ' src="https://gitee.com/circlelq/cat_photos/raw/master/{{item.name}}_circle.png" mode='aspectFill' ></image>
        <view class= "cat-name">{{item.name}}</view> 
      </view>
    </navigator>
  </block>
</view>
